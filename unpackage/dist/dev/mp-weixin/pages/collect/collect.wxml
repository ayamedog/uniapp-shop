<view class="collect"><tabs vue-id="32c8b92a-1" tabs="{{tabList}}" data-event-opts="{{[['^changeActive',[['changeActive']]]]}}" bind:changeActive="__e" bind:__l="__l"></tabs><view class="tags"><block wx:for="{{tagsList}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['changeTagsActive',[index]]]]]}}" class="{{['cu-tag radius bg-red',item.isActive?'bg-red':'bg-white']}}" bindtap="__e">{{item.title}}</view></block></view><block wx:if="{{collect.length!==0}}"><view class="collect_content"><view class="first_tab"><block wx:for="{{collect}}" wx:for-item="item" wx:for-index="index" wx:key="goods_id"><navigator class="goods_item" url="{{'../goods_detail/goods_detail?goods_id='+item.goods_id}}"><view class="goods_img_warp"><image mode="widthFix" src="{{item.goods_small_logo||'https://ww1.sinaimg.cn/large/007rAy9hgy1g24by9t530j30i20i2glm.jpg'}}"></image></view><view class="goods_info_warp"><view class="goods_name">{{item.goods_name}}</view><view class="goods_price text-price text-red">{{item.goods_price}}</view></view></navigator></block></view></view></block><block wx:else><view class="empty"><view>还没有收藏任何商品，快去收藏吧</view><view><navigator url="../category/category" open-type="switchTab"><button class="cu-btn bg-red shadow">去逛逛</button></navigator></view></view></block></view>