<view class="feedback"><tabs vue-id="2d23d9a0-1" tabs="{{tabs}}" data-event-opts="{{[['^changeActive',[['changeActive']]]]}}" bind:changeActive="__e" bind:__l="__l"></tabs><view class="fb_main"><view class="fb_title">问题种类</view><view class="fb_tips"><block wx:for="{{tips}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['changeTipsActive',[index]]]]]}}" class="{{['cu-tag radius',item.isActive?'bg-red':'bg-white']}}" bindtap="__e">{{item.title}}</text></block></view></view><view class="fb_content"><view class="cu-form-group"><textarea maxlength="-1" disabled="{{modalName!=null}}" placeholder="请描述一下您的问题" data-event-opts="{{[['input',[['__set_model',['','text','$event',[]]]]]]}}" value="{{text}}" bindinput="__e"></textarea></view><view class="cu-bar bg-white margin-top"><view class="action">图片上传</view><view class="action">{{''+imgList.length+'/8'}}</view></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:if="{{imgList.length<8}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block></view></view></view><view class="form_btn_warp"><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="cu-btn lg bg-red" bindtap="__e"><text class="cuIcon-check"></text>提交</button></view></view>